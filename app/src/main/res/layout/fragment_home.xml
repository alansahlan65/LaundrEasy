<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
xmlns:tools="http://schemas.android.com/tools"
android:layout_height="match_parent"
tools:context=".HomeFragment">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="457dp"
        android:layout_height="340dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/welcome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Selamat Datang di LaundrEasy"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="330dp"
        android:background="@drawable/rounded"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.85" />

    <View
        android:id="@+id/vLayanan"
        android:layout_width="160dp"
        android:layout_height="116dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/rounded"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="@id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <View
        android:id="@+id/vInformasi"
        android:layout_width="41dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/vPesanan"
        android:layout_width="160dp"
        android:layout_height="116dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/rounded"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <View
        android:id="@+id/vRiwayat"
        android:layout_width="160dp"
        android:layout_height="116dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/rounded"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2" />

    <View
        android:id="@+id/vProfil"
        android:layout_width="160dp"
        android:layout_height="116dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/rounded"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="153dp"
        android:layout_height="87dp"
        android:src="@drawable/ic_baseline_local_laundry_service_24"
        app:layout_constraintEnd_toEndOf="@+id/vLayanan"
        app:layout_constraintStart_toStartOf="@+id/vLayanan"
        app:layout_constraintTop_toTopOf="@+id/vLayanan" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="39dp"
        android:layout_height="29dp"
        android:src="@drawable/ic_baseline_info_24"
        app:layout_constraintBottom_toBottomOf="@+id/vInformasi"
        app:layout_constraintEnd_toEndOf="@+id/vInformasi"
        app:layout_constraintStart_toStartOf="@+id/vInformasi"
        app:layout_constraintTop_toTopOf="@+id/vInformasi" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="153dp"
        android:layout_height="87dp"
        android:src="@drawable/ic_baseline_profile_24"
        app:layout_constraintEnd_toEndOf="@+id/vProfil"
        app:layout_constraintStart_toStartOf="@+id/vProfil"
        app:layout_constraintTop_toTopOf="@+id/vProfil" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="153dp"
        android:layout_height="87dp"
        android:src="@drawable/ic_baseline_riwayat_24"
        app:layout_constraintEnd_toEndOf="@+id/vLayanan"
        app:layout_constraintStart_toStartOf="@+id/vLayanan"
        app:layout_constraintTop_toTopOf="@+id/vRiwayat" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="153dp"
        android:layout_height="87dp"
        android:src="@drawable/ic_baseline_add_shopping_cart_24"
        app:layout_constraintEnd_toEndOf="@+id/vPesanan"
        app:layout_constraintStart_toStartOf="@+id/vPesanan"
        app:layout_constraintTop_toTopOf="@+id/vLayanan" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Layanan"
        android:textSize="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/vLayanan"
        app:layout_constraintStart_toStartOf="@+id/vLayanan"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Riwayat"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/vLayanan"
        app:layout_constraintStart_toStartOf="@+id/vLayanan"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/tvHarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profil"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/vProfil"
        app:layout_constraintStart_toStartOf="@+id/vProfil"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pesanan"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/vPesanan"
        app:layout_constraintStart_toStartOf="@+id/vPesanan"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />


</androidx.constraintlayout.widget.ConstraintLayout>